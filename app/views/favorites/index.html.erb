<p id="notice"><%= notice %></p>

<h1>favorites</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favorites.each do |favorite| %>
      <tr>
        <td><%= image_tag favorite.feed.image.url %></td>
        <td><%= favorite.feed.content %></td>
        <td><%= favorite.user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>